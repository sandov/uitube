{
    "manifest_version": 2,
    "name": "uitube",
    "version": "2024.5.21.3",

    "description": "Make Youtube's terrible new UI slightly less bad",

    "icons":{
    	"48": "icons/uitube-48.png",
        "96": "icons/uitube-96.png"
    },

    "content_scripts":[
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["uitube.js"]
        }
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "__uitube@diegosandoval.net"
		}
    }
}
